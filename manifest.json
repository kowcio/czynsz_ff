{
  "description": "Adds a solid red border to all webpages matching mozilla.org.",
  "manifest_version": 2,
  "name": "Czynsz_FF_estate_care",
  "version": "1.0.0",
  "homepage_url": "https://www.perplexity.ai/",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["app22.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "borderify@example.com"
    }
  },
  "icons": {
    "16": "public/favicon.ico",
    "32": "public/favicon.ico",
    "48": "public/favicon.ico"
  },
  "browser_action": {
    "default_icon": "assets/logo.svg",
    "default_title": "Opens MyExtension",
    "default_popup": "app.html"
  },
  "chrome_url_overrides": {},
  "permissions": ["tabs", "activeTab", "bookmarks", "clipboardRead", "storage"]
}
