{
  "name": "vue-project",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@} --clean --verbose\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:8080 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 8080' http://localhost:8080 'cypress open --e2e'",
    "build-only": "vite build",
    "build2": "vite build --outDir dis --include src/**/*.{js,ts,vue} --verbose",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@quasar/vite-plugin": "^1.9.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "dayjs": "^1.11.7",
    "@vueuse/nuxt": "^13.5.0",
    "@vueuse/core": "^13.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@types/firefox-webext-browser": "^78.15.0",
    "cypress": "^14.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "rollup-plugin-copy": "3.5.0",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.0",
    "vite": "^5.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-web-extension": "4.4.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
